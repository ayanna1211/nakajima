<!DOCTYPE html>
<html>
<head>
  <title><%= yield(:title) %> | Ayanna's blog</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <%= render :partial => "_partials/header"%>
  <div class="wrapper">
    <%= yield %>
  </div>
  <%= render :partial => "_partials/menu"%>
  <%= render :partial => "_partials/footer"%>  
</body>
</html>
